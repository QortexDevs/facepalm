<div class="index-page">
    {% for sectionName, sectionInfo in cmsStructure %}
        {% if sectionInfo.sections %}
            {% for subSectionName, subSectionInfo in sectionInfo.sections %}
                <div class="tile" style="background-color: {{ palette[loop.parent.loop.index0] }}">
                    {% if sectionInfo.icon %}<i class="fa fa-fw fa-{{ sectionInfo.icon }}"></i>{% endif %}
                    <a href="/cms/{{ sectionName }}/{{ subSectionName }}/">
                        <span>{{ subSectionInfo.title }}</span>
                        <span class="top-level-name">{{ sectionInfo.title }}</span>
                    </a>
                </div>
            {% endfor %}
        {% endif %}
    {% endfor %}
    {% for i in 1..10 %}
        <div class="tile empty"></div>
    {% endfor %}
</div>

<tr class="cms-nested-header">
    {% for field in form.fields %}
        {% if not field.isSkipped %}
            {% if field.isHidden %}
                {{ field.renderFormField(form.object)|raw }}
            {% else %}
                <th>{{ field.title }}</th>
            {% endif %}
        {% endif %}
    {% endfor %}
    <th></th>
</tr>

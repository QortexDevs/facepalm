{
    "name": "xpundel/facepalm",
    "description": "The FacepalmCMS.",
    "keywords": [
        "facepalm",
        "cms"
    ],
    "authors": [
        {
            "name": "Mikhail Lisnyak",
            "email": "xpundel@gmail.com"
        }
    ],

    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "twig/twig": "~1.23",
        "rcrowe/twigbridge": "0.9.x",
        "intervention/image": "^2.3",
        "hiroto-k/json5-php": "~1.0",
        "cocur/slugify": "^2.4",
        "doctrine/dbal": "^2.6"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.3",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "squizlabs/php_codesniffer": "2.*",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Facepalm\\": "src/Facepalm/"
        },
        "files": [
            "src/Facepalm/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}

{
    "name": "xpundel/facepalm",
    "description": "The FacepalmCMS.",
    "keywords": [
        "facepalm",
        "cms"
    ],
    "authors": [
        {
            "name": "Mikhail Lisnyak",
            "email": "xpundel@gmail.com"
        }
    ],
    "require": {
        "twig/twig": "^2",
        "rcrowe/twigbridge": "^0.12",
        "intervention/image": "^2.3",
        "cocur/slugify": "^2.4",
        "doctrine/dbal": "^2.6",
        "php": "^7.2.5",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^7.22.2",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.1",
        "colinodell/json5": "^2.1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.3",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "2.*",
        "phpspec/phpspec": "^6.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Facepalm\\": "src/Facepalm/"
        },
        "files": [
            "src/Facepalm/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}

{
    "name": "xpundel/facepalmcms",
    "description": "The FacepalmCMS.",
    "keywords": ["facepalm", "cms"],
    "authors": [
        {
            "name": "Mikhail Lisnyak",
            "email": "xpundel@gmail.com"
        }
    ],
    "require": {
 	   "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "twig/twig": "~1.23",
        "rcrowe/twigbridge": "0.9.x",
        "intervention/image": "^2.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
       "psr-4": {
            "Facepalm\\": "src/Facepalm/",
            "Facepalm\\Tests\\": "tests"
       },
        "files": [
            "src/Facepalm/helpers.php"
        ]
    }
}
